@model IEnumerable<terceirizados_novo.Models.Servico>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NomeServico)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DescricaoServico)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Orcamento)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ValorEstimadoServico)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DataAprovacaoOrcamento)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DataConclusaoDoServico)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NomeServico)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DescricaoServico)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Orcamento)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ValorEstimadoServico)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DataAprovacaoOrcamento)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DataConclusaoDoServico)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
